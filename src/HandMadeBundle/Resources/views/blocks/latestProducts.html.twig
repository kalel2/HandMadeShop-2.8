<div class="row latest-products">
    {% block body %}
        {% for product in products %}
            <div class="col-md-4 col-sm-4 products">
                {% set aproductImage = vich_uploader_asset(product, 'imageFile') %}
                <img class="thumbnail" src="{{ asset(['bundles/handmade/', aproductImage]|join) }}"  />
                <h4> {{ product.name | trans }}</h4>
                <h5> {{ product.price | number_format(2, ',', ' ') }}</h5>
            </div>
        {% endfor %}
    {% endblock %}
</div>
